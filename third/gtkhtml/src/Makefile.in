install-keybindingsDATA: $(keybindings_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(keybindingsdir)
	@list='$(keybindings_DATA)'; for p in $$list; do \
	  if test -f $(srcdir)/$$p; then \
	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(keybindingsdir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(keybindingsdir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(keybindingsdir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(keybindingsdir)/$$p; \
	  fi; fi; \
	done

