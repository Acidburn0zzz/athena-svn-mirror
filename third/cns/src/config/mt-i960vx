# /* vxworks */

# Host support files -- sort of Unix-like?!  (Untested)
DES_TIME_SRCS=unix_time.c
DES_TIME_OBJS=unix_time.o
LIB_KRB_HOSTSRCS=unix_glue.c
LIB_KRB_HOSTOBJS=unix_glue.o

TARGET_CDEFS= -DBSD42 -DATHENA -DHAVE_VSPRINTF \
	-DHAS_STRDUP -DWAIT_USES_INT -DNO_YYLINENO -DPOSIX \
	-DNO_SIGCONTEXT \
	-DNOTTYENT -DNO_LASTLOG
# vxworks has setenv/unsetenv in libc
SETENVSRCS=
SETENVOBJS=
BSDLIB=
LEXLIB=
LIBNDBM=
RPROGS=-DRPROGS_IN_USR_BIN
DEPEND=true
