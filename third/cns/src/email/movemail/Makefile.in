#### insert configury here
BUILDTOP=$(C)$(S)$(U)$(U)
DEFINES=-DBSD -DMAIL_USE_POP $(FLOCK) -DKERBEROS
LIBS=${KRB_LIB} ${HESIOD_LIB} ${DES_LIB}
DEPLIBS=${KRB_LIBDEP} ${DES_LIBDEP}

all:: movemail

SRCS=movemail.c

# program(movemail,movemail.o,${DEPLIBS},${LIBS},${PROGDIR})
movemail: movemail.o ${DEPLIBS}
	$(CC) $(CCFLAGS) -o $@ movemail.o ${LIBS}
movemail.o: movemail.c

install::
	$(INSTALLPROG) movemail ${DESTDIR}${PROGDIR}/movemail

clean::
	$(RM) movemail movemail.o

distclean:: clean
	$(RM) *~ core
	$(RM) Makefile config.status 

