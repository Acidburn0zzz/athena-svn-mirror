install::
	-mkdir ${DESTDIR}${INCLDIR}
	for i in ${UHFILES}; do \
		${INSTALLFILE} $$i ${DESTDIR}${INCLDIR}/$$i; done

depend:: $(SRCS)
	@echo "### Now computing dependencies"
	@$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

clean::
	$(RM) *~ \#* *.bak $(TAGSFILE)

tags::
	$(TAGGER) *.c *.h

