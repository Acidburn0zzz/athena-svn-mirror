DEFINES	= -DBSD42

SRCS	= \
	krb_dbm.c \
	krb_lib.c \
	krb_cache.c \
	print_principal.c \
	krb_kdb_utils.c \
	krb_dbl.c

OBJECTS	= \
	krb_dbm.o \
	krb_lib.o \
	krb_cache.o \
	print_principal.o \
	krb_kdb_utils.o \
	krb_dbl.o

SRCDIR=$(SRCTOP)/lib/kdb
CODE= ${SRCS} IMakefile

.c.o:
	$(CC) -p -c $(CFLAGS) $*.c
	-$(LD) -X -r -o profiled/$*.o $*.o
	$(CC) -c $(CFLAGS) $*.c
	-$(LD) -x -r -o a.out $*.o
	$(MV) a.out $*.o

all:: libkdb.a libkdb_p.a
all:: llib-lkdb.ln

libkdb.a: $(OBJECTS)
	$(ARCHIVE) $@ $(OBJECTS)
	$(RANLIB) $@

libkdb_p.a: $(OBJECTS)
	cd profiled; $(ARCHIVE) ../$@ $(OBJECTS)
	$(RANLIB) $@

lint:: llib-lkdb.ln

llib-lkdb.ln: $(SRCS)
	$(LINT) -Ckdb $(LINTFLAGS) $(SRCS) ${DES_LINTLIB} ${KRB_LINTLIB}

clean::
	$(RM) libkdb.a libkdb_p.a
	$(RM) llib-lkdb.ln
	$(RM) $(OBJECTS)
	$(RM) profiled/?*.o

install::
	$(INSTALLRAW) -m 644 libkdb.a $(DESTDIR)$(LIBDIR)/libkdb.a
	$(RANLIB) -t $(DESTDIR)$(LIBDIR)/libkdb.a
	$(CHMOD) 444 $(DESTDIR)$(LIBDIR)/libkdb.a
	$(INSTALLRAW) -m 644 libkdb_p.a $(DESTDIR)$(LIBDIR)/libkdb_p.a
	$(RANLIB) -t $(DESTDIR)$(LIBDIR)/libkdb_p.a
	$(CHMOD) 444 $(DESTDIR)$(LIBDIR)/libkdb_p.a
	$(INSTALLFILE) llib-lkdb.ln $(DESTDIR)$(LINTLIBDIR)/llib-lkdb.ln

depend:: $(SRCS)
	@echo "### Now computing dependencies"
	@$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

clean::
	$(RM) *~ \#* *.bak $(TAGSFILE)

tags::
	$(TAGGER) *.c *.h

Makefile::
	-$(RM) Makefile.bak; $(MV) Makefile Makefile.bak
	$(IMAKE) -DNEW_TOP=$(NEWTOP) -s Makefile

print:: ${CODE}
	${LPR} ${CODE}

src:: ${CODE}

${CODE}:
	$(RM) $@
	-$(LN) ${SRCDIR}/$@ $@

install::

