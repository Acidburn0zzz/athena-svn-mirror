#### include configury here
BUILDTOP=$(C)$(S)$(U)$(U)

SRCS	= \
	acl_files.c

OBJS	= \
	acl_files.o

CODE= ${SRCS} Makefile.in

# install_library_target(acl,$(OBJECTS),$(SRCS),)
# install_library_target(ss,$(OBJS),$(SRCS),)
all:: libacl.$(LIBEXT)

libacl.$(LIBEXT): $(OBJS)
	$(RM) $@
	$(ARCHIVE) $@ $(OBJS)
	$(RANLIB) $@

clean::
	$(RM) libacl.$(LIBEXT)
	$(RM) $(OBJS)

distclean:: clean
	$(RM) *~ core
	$(RM) Makefile config.status 

install::
	$(INSTALLLIB) libacl.$(LIBEXT) $(DESTDIR)$(LIBDIR)/libacl.$(LIBEXT)
	$(CHMOD) 644 $(DESTDIR)$(LIBDIR)/libacl.$(LIBEXT)
	$(RANLIB)    $(DESTDIR)$(LIBDIR)/libacl.$(LIBEXT)
	$(CHMOD) 444 $(DESTDIR)$(LIBDIR)/libacl.$(LIBEXT)
