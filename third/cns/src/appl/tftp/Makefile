SRCS= conn.c resolve_host.c srvr_cmds.c srvr_log.c tcom.c \
	tftp.c tftpd.c tmpnam.c udp.c user_cmds.c user_log.c \
	notice.h tftp.h conn.h srvr_cmds.h
SRCDIR=${SRCTOP}/appl/tftp
CODE=${SRCS} Imakefile man/tftpd.8 man/tcom.8 man/tftp.1
SOBJS=tftpd.o conn.o srvr_log.o srvr_cmds.o tmpnam.o udp.o
COBJS=tftp.o conn.o user_log.o user_cmds.o udp.o resolve_host.o

all:: tftpd

tftpd: ${SOBJS} ${KRB_LIBDEP} ${DES_LIBDEP}
	$(CC) $(CFLAGS) -o $@ ${SOBJS}  \
	${KRB_LIB} ${DES_LIB}

install::
	$(INSTALLRAW)  -c -s -m 4555 -o ${DEF_UID} tftpd ${DESTDIR}${ETCDIR}/tftpd

clean::
	$(RM) tftpd ${SOBJS}

all:: tftp

tftp: ${KRB_LIBDEP} ${DES_LIBDEP} ${COBJS}
	$(CC) $(CFLAGS) -o $@ ${COBJS}  \
	${KRB_LIB} ${DES_LIB}

install::
	$(INSTALLPROG) tftp ${DESTDIR}${ATHENADIR}/tftp

clean::
	$(RM) tftp ${COBJS}

all:: tcom

tcom: tcom.o ${KRB_LIBDEP} ${DES_LIBDEP}
	$(CC) $(CFLAGS) -o $@ tcom.o  \
	${KRB_LIB} ${DES_LIB}

install::
	$(INSTALLRAW)  -c -s -m 6555 -o ${DEF_UID} tcom ${DESTDIR}${ATHENADIR}/tcom

clean::
	$(RM) tcom tcom.o

all::
install:: man/tftpd.8
	$(INSTALLFILE) man/tftpd.8 ${DESTDIR}${MANDIR}/man8/`basename man/tftpd.8`

all::
install:: man/tcom.8
	$(INSTALLFILE) man/tcom.8 ${DESTDIR}${MANDIR}/man8/`basename man/tcom.8`

all::
install:: man/tftp.1
	$(INSTALLFILE) man/tftp.1 ${DESTDIR}${MANDIR}/man1/`basename man/tftp.1`

depend:: $(SRCS)
	@echo "### Now computing dependencies"
	@$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

clean::
	$(RM) *~ \#* *.bak $(TAGSFILE)

tags::
	$(TAGGER) *.c *.h

Makefile::
	-$(RM) Makefile.bak; $(MV) Makefile Makefile.bak
	$(IMAKE) -DNEW_TOP=$(NEWTOP) -s Makefile

print:: ${CODE}
	${LPR} ${CODE}

src:: ${CODE}

