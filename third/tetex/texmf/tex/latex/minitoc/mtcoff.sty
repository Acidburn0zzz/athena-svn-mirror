\newcommand{\mtcaddchapter}[1][]{\mtcoffwarn@true%                %v33
\PackageWarning{mtcoff}{\protect\mtcaddchapter{...} should be     %v33
replaced\MessageBreak%                                            %v33
by \protect\addcontentsline{toc}{chapter}{...}}                   %v33
\def\mtc@ck{#1}                                                   %v33
\ifx\mtc@ck\empty                                                 %v33
\else                                                             %v33
\addcontentsline{toc}{chapter}{#1}\fi}                            %v33
\newcommand{\mtcaddsection}[1][]{\mtcoffwarn@true%                %v33
\PackageWarning{mtcoff}{\protect\mtcaddsection{...} should be     %v33
replaced\MessageBreak%                                            %v33
by \protect\addcontentsline{toc}{section}{...}}                   %v33
\def\mtc@ck{#1}                                                   %v33
\ifx\mtc@ck\empty                                                 %v33
\else                                                             %v33
\addcontentsline{toc}{part}{#1}\fi}                               %v33
\newcommand{\mtcaddpart}[1][]{\mtcoffwarn@true%                   %v33
