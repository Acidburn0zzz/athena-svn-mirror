\relax

% OT1i.etx -- install an OT1-encoded italic font
%
% We _do_ need to distinguish between roman and italic in OT1,
% because of `sterling' and `dollar' sharing the same slot.

\encoding

\setcommand\lc#1#2{#2}
\setcommand\uc#1#2{#1}
\setcommand\lclig#1#2{#2}
\setcommand\uclig#1#2{#1}
\setcommand\digit#1{#1}
\setcommand\currency{sterling}

\inputetx{OT1}

\endencoding

