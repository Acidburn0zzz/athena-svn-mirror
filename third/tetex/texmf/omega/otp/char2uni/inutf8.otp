% The information was provided by Martin Duerst.
%

input:  1;
output: 2;

expressions:

@"00-@"7F
  => \1;
(@"C0-@"DF)(@"80-@"BF)
  => #(((\1-@"C0)*@"40) + (\2-@"80));
(@"E0-@"EF)(@"80-@"BF)(@"80-@"BF)
  => #(((\1-@"E0)*@"1000) + ((\2-@"80)*@"40) + (\3-@"80));
. => @"FFFD;
