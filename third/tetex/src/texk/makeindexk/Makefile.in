# Makefile for makeindexk --te@informatik.uni-hannover.de. Public domain.
version = 2.13

ac_include ../make/paths.mk
ac_include ../make/common.mk
ac_include ../make/programs.mk

objects = genind.o mkind.o qsort.o scanid.o scanst.o sortid.o
program = makeindex
programs = $(program)

default all: $(programs)

$(program): $(kpathsea) $(objects)
	$(kpathsea_link) $(objects) $(LOADLIBES)
ac_include ../make/tkpathsea.mk

install: install-exec install-data
uninstall: uninstall-exec uninstall-data

install-exec: $(programs)
	$(SHELL) $(top_srcdir)/../mkinstalldirs ${DESTDIR}$(bindir)
	$(INSTALL_LIBTOOL_PROG) ${DESTDIR}$(bindir) $(program)
	$(INSTALL_SCRIPT) $(srcdir)/mkindex ${DESTDIR}$(scriptdir)/mkindex
uninstall-exec:
	cd $(bindir) && rm -f $(program)
	cd $(scriptdir) && rm -f mkindex

install-data: makeindex.1
	$(SHELL) $(top_srcdir)/../mkinstalldirs ${DESTDIR}$(mandir)
	$(INSTALL_DATA) $(srcdir)/makeindex.1 ${DESTDIR}$(mandir)/makeindex.$(manext)

uninstall-data:

distname = makeindex
program_files =
version_files =

pre-dist-$(distname):
post-dist-$(distname):

ac_include ../make/dist.mk
ac_include ../make/config.mk

info dvi check:

ac_include ../make/clean.mk

distclean::

ac_include ../make/rdepend.mk
ac_include depend.mk
