# $Id: Makefile.athena,v 1.3 1999-04-08 19:10:57 ghudson Exp $

SHELL=/bin/sh
BUILD=`pwd`/BuildTools/bin/Build

prepare:

clean:
	${MAKE} BUILD=$(BUILD) clean

all:
	${MAKE} BUILD=$(BUILD) CC=$$COMPILER all

check:

install:
	mkdir -p $$SRVD/usr/lib
	${MAKE} BUILD=$(BUILD) CC=$$COMPILER DESTDIR=$$SRVD install
