# $Id: Makefile.athena,v 1.6 1999-06-16 16:28:46 danw Exp $

SHELL=/bin/sh
BUILD=`pwd`/BuildTools/bin/Build

prepare:
	rm -rf */obj.*

clean:
	${MAKE} BUILD=$(BUILD) clean

all:
	${MAKE} BUILD=$(BUILD) CC=$$COMPILER MAKE=$(MAKE) all

check:

install:
	${MAKE} BUILD=$(BUILD) CC=$$COMPILER DESTDIR=$$SRVD install
