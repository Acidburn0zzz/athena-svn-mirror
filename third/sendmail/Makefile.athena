SHELL=/bin/sh
BUILD=`pwd`/devtools/bin/Build

dist:

prepare:
	rm -rf obj.*

clean:
	${MAKE} BUILD=$(BUILD) clean

all:
	${MAKE} BUILD=$(BUILD) CC=$$COMPILER all

check:

install:
	${MAKE} BUILD=$(BUILD) CC=$$COMPILER DESTDIR=$$SRVD install
