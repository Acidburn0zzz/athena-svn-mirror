#: libnautilus-private/nautilus-search-uri.c:333
#, c-format
msgid "[Items ]not modified on %s"
msgstr "[항목 ]%s에 바뀌지 않음"

#: libnautilus-private/nautilus-search-uri.c:336
#, c-format
msgid "[Items ]modified before %s"
msgstr "[항목 ]%s 이전에 바뀜"

#: libnautilus-private/nautilus-search-uri.c:339
#, c-format
