SHELL=/bin/sh

CC=$$CCOMP

CFLAGS=$$CCFLAGS

LFLAGS=$$CAPLIB

programs: capcomm

capcomm: capcomm.o psutil.o
	${CC} ${CFLAGS} -o capcomm capcomm.o psutil.o ${LFLAGS} -lcap









