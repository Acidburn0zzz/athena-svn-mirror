# $Id: Makefile.in,v 1.2 1998-12-27 22:42:31 rbasch Exp $

SHELL=/bin/sh
INSTALL=@INSTALL@
VPATH=@srcdir@
srcdir=@srcdir@
top_srcdir=@top_srcdir@
prefix=@prefix@
exec_prefix=@exec_prefix@
lsbindir=@lsbindir@
mandir=@mandir@

CC=@CC@
CPPFLAGS=@CPPFLAGS@
DEFS=@DEFS@
WRAP_INCLUDES=@WRAP_INCLUDES@
CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@
WRAP_LIB=@WRAP_LIB@
ALL_CFLAGS=-I. ${WRAP_INCLUDES} ${CPPFLAGS} ${DEFS} ${CFLAGS}

all: inetd

inetd: inetd.o
	${CC} ${LDFLAGS} -o inetd inetd.o ${WRAP_LIB} ${LIBS}

.c.o:
	${CC} -c ${ALL_CFLAGS} $<

check:

install:
	${top_srcdir}/mkinstalldirs ${DESTDIR}${lsbindir}
	${top_srcdir}/mkinstalldirs ${DESTDIR}${mandir}/man1
	${INSTALL} -m 555 inetd ${DESTDIR}${lsbindir}
	${INSTALL} -m 444 ${srcdir}/inetd.8 ${DESTDIR}${mandir}/man8
	${INSTALL} -m 444 ${srcdir}/inetd.conf.5 ${DESTDIR}${mandir}/man5

clean:
	rm -f inetd.o inetd

distclean: clean
	rm -f config.cache config.log config.status Makefile
