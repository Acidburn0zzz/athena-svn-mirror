#!/bin/sh -
#
# $Id: chk.offt,v 1.1.1.1 2002-02-11 16:24:49 ghudson Exp $
#
# Make sure that no off_t's have snuck into the release.

egrep -w off_t ../*/*.[ch] |
sed -e '/build_/d' \
    -e '/os\//d' \
    -e '/:[	 ][	 ]*\* /d' \
    -e '/os_seek.c:.*off_t offset;/d' \
    -e '/os_seek.c:.*offset = /d'
