# $Id: Makefile.athena,v 1.2 1998-06-24 01:54:37 ghudson Exp $

SHELL=/bin/sh

prepare:

clean:
	cd src && ${MAKE} clean

all:
	cd src && ${MAKE} "CC=$$COMPILER" CDEBUG=-g \
		DESTBIN=/usr/athena/bin \
		DESTSBIN=/etc/athena \
		DESTEXEC=/etc/athena \
		DESTMAN=/usr/athena/man \
		DESTHELP=/usr/athena/lib

check:

install:
	cd src && ${MAKE} install "DESTDIR=$$SRVD"

