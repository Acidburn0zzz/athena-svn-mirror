<oaf_info>

<!-- WHAT YOU NEED TO CHANGE:

  a) Replace all the OAFIID strings to do this you need to register
     your namespace, and then generate some unique names inside
     it. See bonobo/doc/FAQ 'How should I create unique OAFIIDs for
     components ?'.

  b) Alter the repo_ids to the interfaces you support.

  c) Point the 'location' at the executable that your project installs
     @LIBEXECDIR@ will get expanded properly, only change the binary name

  d) And finally, make sure to remove the test_only attribute.
     In this Server file it has been set to false which works as well.
  -->

<oaf_server iid="OAFIID:Nautilus_Test_View_Factory"
	type="exe" location="@LIBEXECDIR@/nautilus-test-view">
	<oaf_attribute name="repo_ids" type="stringv">
		<item value="IDL:GNOME/ObjectFactory:1.0"/>
	</oaf_attribute>
	<oaf_attribute name="name" type="string" _value="Nautilus Test view factory"/>
	<oaf_attribute name="description" type="string" _value="Test view component's factory"/>
</oaf_server>

<oaf_server iid="OAFIID:Nautilus_Test_View" type="factory" location="OAFIID:Nautilus_Test_View_Factory">
	<oaf_attribute name="test_only" type="boolean" value="false"/>
	<oaf_attribute name="repo_ids" type="stringv">
		<item value="IDL:Bonobo/Unknown:1.0"/>
		<item value="IDL:Bonobo/Control:1.0"/>
		<item value="IDL:Nautilus/View:1.0"/>
	</oaf_attribute>
	<oaf_attribute name="name" type="string" _value="Nautilus Test view"/>
	<oaf_attribute name="description" type="string" _value="Test view component"/>
	<oaf_attribute name="bonobo:additional_uri_schemes" type="stringv">
		<item value="*"/>
	</oaf_attribute>
	<oaf_attribute name="bonobo:supported_mime_types" type="stringv">
		<item value="*"/>
	</oaf_attribute>
	<oaf_attribute name="nautilus:view_as_name" type="string" _value="Test"/>
	<oaf_attribute name="nautilus:view_as_label" type="string" _value="View as Test"/>
	<oaf_attribute name="nautilus:viewer_label" type="string" _value="Test Viewer"/>
</oaf_server>

</oaf_info>
