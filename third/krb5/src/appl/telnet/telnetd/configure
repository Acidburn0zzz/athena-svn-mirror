s%@CHECK_RECURSE@%$CHECK_RECURSE%g
s%@MAKEFILES_RECURSE@%$MAKEFILES_RECURSE%g
s%@INSTALL_PROGRAM@%$INSTALL_PROGRAM%g
s%@INSTALL_DATA@%$INSTALL_DATA%g
