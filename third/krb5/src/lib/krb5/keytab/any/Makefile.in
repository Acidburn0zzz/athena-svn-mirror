CFLAGS = $(CCOPTS) $(DEFS)

##DOSBUILDTOP = ..\..\..\..
##DOSLIBNAME=..\..\krb5.lib

.c.o:
	$(CC) $(CFLAGS) -c $(srcdir)/$*.c
@SHARED_RULE@

SRCS= \
	$(srcdir)/kta_ops.c \
	$(srcdir)/kta_resolv.c

OBJS = \
	kta_ops.$(OBJEXT)	\
	kta_resolv.$(OBJEXT)

all-unix:: shared $(OBJS)
all-mac:: $(OBJS)
all-windows:: $(OBJS)

shared:
	mkdir shared

clean-unix::
	$(RM) shared/*

clean-mac::
clean-windows::
