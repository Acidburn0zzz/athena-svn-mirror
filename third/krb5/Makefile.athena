SHELL=/bin/sh

configure:
	rm -f config.cache
	./configure

clean:
	${MAKE} clean

all:
	${MAKE} depend all

check:

install:
	cd include && ${MAKE} install DESTDIR="$$SRVD"
	cd lib/krb5 && ${MAKE} install DESTDIR="$$SRVD"
	cd lib/crypto && ${MAKE} install DESTDIR="$$SRVD"
