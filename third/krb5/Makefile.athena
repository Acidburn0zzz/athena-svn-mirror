# $Id: Makefile.athena,v 1.4 1996-10-13 20:36:56 ghudson Exp $

SHELL=/bin/sh

configure: force
	rm -f src/config.cache
	cd src && ./configure

force:

clean:
	cd src && ${MAKE} clean

all:
	cd src && ${MAKE} depend all

check:

install:
	cd src/include && ${MAKE} install DESTDIR="$$SRVD"
	cd src/lib/krb5 && ${MAKE} install DESTDIR="$$SRVD"
	cd src/lib/crypto && ${MAKE} install DESTDIR="$$SRVD"
