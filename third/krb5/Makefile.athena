# $Id: Makefile.athena,v 1.2 1996-10-13 04:28:40 ghudson Exp $

SHELL=/bin/sh

configure: force
	rm -f config.cache
	./configure

force:

clean:
	${MAKE} clean

all:
	${MAKE} depend all

check:

install:
	cd include && ${MAKE} install DESTDIR="$$SRVD"
	cd lib/krb5 && ${MAKE} install DESTDIR="$$SRVD"
	cd lib/crypto && ${MAKE} install DESTDIR="$$SRVD"
