# $Id: Makefile.athena,v 1.15 1998-12-20 05:40:39 ghudson Exp $

SHELL=/bin/sh
ATHETCDIR=/usr/athena/etc
ATHINCDIR=/usr/athena/include

prepare:
	rm -f src/config.cache
	cd src && CPPFLAGS=" " ./configure --with-krb4=yes

clean:
	cd src && ${MAKE} clean

all:
	cd src && ${MAKE} depend all

check:

install:
	cd src && ${MAKE} install DESTDIR="$$SRVD"
