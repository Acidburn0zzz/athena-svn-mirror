# $Id: Makefile.athena,v 1.5 2004-04-08 18:05:18 rbasch Exp $

dist:
	cp "$$CONFIG_SITE" config.site.athena

prepare:
	rm -f config.cache
	CONFIG_SITE=`pwd`/config.site.athena ./configure

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	libgcc=`$$COMPILER -print-libgcc-file-name` ; \
	libdir=`dirname $$libgcc` ; \
	mkdir -p $$SRVD$$libdir ; \
	install -c ld/ld-new $$SRVD$$libdir/ld
