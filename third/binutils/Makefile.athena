# $Id: Makefile.athena,v 1.3 2002-02-03 15:26:47 ghudson Exp $

dist:
	cp "$$CONFIG_SITE" config.site.athena

prepare:
	rm -f config.cache
	CONFIG_SITE=`pwd`/config.site.athena ./configure

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	install -c ld/ld-new \
	  `$$COMPILER -v 2| sed -ne 's|.* \(.*\)/specs|\1|p'`/ld
