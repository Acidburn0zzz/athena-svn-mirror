# $Id: Makefile.athena,v 1.6 2004-04-16 04:14:44 ghudson Exp $

dist:
	cp "$$CONFIG_SITE" config.site.athena

prepare:
	rm -f config.cache
	CONFIG_SITE=`pwd`/config.site.athena LEX=lex ./configure --disable-nls

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	libgcc=`$$COMPILER -print-libgcc-file-name` ; \
	libdir=`dirname $$libgcc` ; \
	mkdir -p $$SRVD$$libdir ; \
	install -c ld/ld-new $$SRVD$$libdir/ld
