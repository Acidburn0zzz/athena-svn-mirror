	$(mkinstalldirs) $(DESTDIR)$(serverdir)
	$(INSTALL_DATA) $(srcdir)/GNOME_Evolution_TestComponent.server $(DESTDIR)$(serverdir)/GNOME_Evolution_TestComponent.server
%.server.in: %.server.in.in
	sed -e "s|\@BINDIR\@|$(bindir)|"				\
	    -e "s|\@VERSION\@|$(BASE_VERSION)|"				\
	    $< > $@

@INTLTOOL_SERVER_RULE@

install-data-local:
	if test -z "$(DESTDIR)" ; then												\
		for p in $(schema_DATA) ; do											\
			GCONF_CONFIG_SOURCE=$(GCONF_SCHEMA_CONFIG_SOURCE) $(GCONFTOOL) --makefile-install-rule $(srcdir)/$$p;	\
		done														\
	fi

install-evolution:
	$(mkinstalldirs) $(DESTDIR)$(bindir)
	$(LIBTOOL) --mode=install $(INSTALL_PROGRAM) evolution $(DESTDIR)$(bindir)/evolution-$(BASE_VERSION)
@DEFAULT_BINARY_TRUE@	cd $(DESTDIR)$(bindir) && rm -f evolution && $(LN_S) evolution-$(BASE_VERSION) evolution

@HAVE_DTAPPINTEGRATE_TRUE@install-exec-local: install-evolution
@HAVE_DTAPPINTEGRATE_TRUE@	$(mkinstalldirs) $(DESTDIR)$(libexecdir)
@HAVE_DTAPPINTEGRATE_TRUE@	mv $(DESTDIR)$(bindir)/evolution-$(BASE_VERSION) $(DESTDIR)$(libexecdir)/evolution-$(BASE_VERSION)
@HAVE_DTAPPINTEGRATE_TRUE@	$(INSTALL_PROGRAM) evolution-nognome $(DESTDIR)$(bindir)/evolution-$(BASE_VERSION) 

@HAVE_DTAPPINTEGRATE_FALSE@install-exec-local: install-evolution

# GLib marshalling cruft

e-shell-marshal.h: e-shell-marshal.list
	( @GLIB_GENMARSHAL@ --prefix=e_shell_marshal e-shell-marshal.list --header > e-shell-marshal.h.tmp \
	&& mv e-shell-marshal.h.tmp e-shell-marshal.h ) \
	|| ( rm -f e-shell-marshal.h.tmp && exit 1 )

e-shell-marshal.c: e-shell-marshal.h
	( @GLIB_GENMARSHAL@ --prefix=e_shell_marshal e-shell-marshal.list --body > e-shell-marshal.c.tmp \
	&& mv e-shell-marshal.c.tmp e-shell-marshal.c ) \
	|| ( rm -f e-shell-marshal.c.tmp && exit 1 )

@ENABLE_PURIFY_TRUE@all-local: evolution.pure

@ENABLE_PURIFY_TRUE@evolution.pure: evolution
@ENABLE_PURIFY_TRUE@	@rm -f evolution.pure
@ENABLE_PURIFY_TRUE@	$(PLINK) $(evolution_LDFLAGS) $(evolution_OBJECTS) $(evolution_LDADD) $(LIBS)

dist-hook:
	cd $(distdir); rm -f $(BUILT_SOURCES)

