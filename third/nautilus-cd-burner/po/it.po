#: nautilus-cd-burner.c:284
msgid "Preparing to write CD"
msgstr "Preparazione per la scrittura del CD in corso"

#: nautilus-cd-burner.c:286
msgid "Preparing to write DVD"
msgstr "Preparazione per la scrittura del DVD in corso"

#: nautilus-cd-burner.c:291
msgid "Writing CD"
msgstr "Scrittura del CD in corso"

#: nautilus-cd-burner.c:293
msgid "Writing DVD"
msgstr "Scrittura del DVD in corso"

#: nautilus-cd-burner.c:298
msgid "Fixating CD"
msgstr "Chiusura del CD in corso"

#: nautilus-cd-burner.c:300
msgid "Fixating DVD"
msgstr "Chiusura del DVD in corso"

#: nautilus-cd-burner.c:305
msgid "Erasing CD"
msgstr "Cancellazione del CD in corso"

#: nautilus-cd-burner.c:307
msgid "Erasing DVD"
msgstr "Cancellazione del DVD in corso"

#: nautilus-cd-burner.c:333
msgid "Please make sure another application is not using the disc."
msgstr "Assicurarsi che nessun altra applicazione stia usando il disco."

#: nautilus-cd-burner.c:334
msgid "Disc is busy"
msgstr "Il disco è occupato"

#: nautilus-cd-burner.c:336
msgid "Please insert a rewritable or blank media in the drive tray."
msgstr "Inserire un supporto riscrivibile o vergine nel carrello dell'unità."

#: nautilus-cd-burner.c:337
msgid "Insert rewritable or blank media"
msgstr "Inserire un supporto riscrivibile o vergine"

#: nautilus-cd-burner.c:339
msgid "Please insert a blank media in the drive tray."
msgstr "Inserire un supporto vergine nel carrello dell'unità."

#: nautilus-cd-burner.c:340
msgid "Insert blank media"
msgstr "Inserire un supporto vergine"

#: nautilus-cd-burner.c:342
msgid "Please replace the in-drive media by a rewritable or blank media."
msgstr "Sostituire il supporto del drive con uno riscrivibile o vergine."

#: nautilus-cd-burner.c:343
msgid "Reload rewritable or blank media"
msgstr "Ricaricare supporto riscrivibile o vergine"

#: nautilus-cd-burner.c:345
msgid "Please replace the in-drive media by a blank media."
msgstr "Sostituire il supporto nell'unità con uno vergine."

#: nautilus-cd-burner.c:346
msgid "Reload blank media"
msgstr "Ricaricare supporto vergine"

