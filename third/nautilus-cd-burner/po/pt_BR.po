msgstr "*"

#: cdburn.glade.h:3
msgid "<b>After Writing</b>"
msgstr "<b>Depois da Gravação</b>"

#: cdburn.glade.h:4
msgid "<b>Information</b>"
msgstr "<b>Informações</b>"

#: cdburn.glade.h:5
msgid "<b>Write Options</b>"
msgstr "<b>Opções de Gravação</b>"

#: cdburn.glade.h:6
msgid "<big><b>Writing files to disc progress</b></big>"
msgstr "<big><b>Progresso da gravação de arquivos no disco</b></big>"

#: cdburn.glade.h:7
msgid "<i>Initializing...</i>"
msgstr "<i>Iniciando...</i>"

#: cdburn.glade.h:8
msgid "Data size:"
msgstr "Tamanho dos dados:"

