msgstr "<b>Informácie</b>"

#: cdburn.glade.h:5
msgid "<b>Write Options</b>"
msgstr "<b>Voľby zápisu</b>"

#: cdburn.glade.h:6
msgid "<big><b>Writing files to disc progress</b></big>"
msgstr "<big><b>Postup zápisu súborov na disk</b></big>"

#: cdburn.glade.h:7
msgid "<i>Initializing...</i>"
msgstr "<i>Inicializácia...</i>"

#: cdburn.glade.h:8
msgid "Data size:"
msgstr "Veľkosť dát:"

