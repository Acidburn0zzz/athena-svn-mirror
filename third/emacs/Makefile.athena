# $Id: Makefile.athena,v 1.4 1997-03-11 19:20:08 ghudson Exp $

SHELL=/bin/sh

prepare:
	rm -f config.cache
	./configure --with-x-toolkit --sharedstatedir=/var/tmp \
		--datadir=/usr/athena/share --with-pop --with-hesiod \
		--with-kerberos

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	${MAKE} install "DESTDIR=$$SRVD"
