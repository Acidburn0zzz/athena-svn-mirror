# $Id: Makefile.athena,v 1.3 1997-09-24 07:07:33 ghudson Exp $

SHELL=/bin/sh

prepare:
	cd conf && $$COMPILER mhconfig.c -o mhconfig
	cd conf && rm -f MH
	cd conf && sed -e "s,@TOOLROOT@,$$ATHTOOLROOT," "MH.$$HOSTTYPE" > MH
	cd conf && ./mhconfig MH

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	${MAKE} install "DESTDIR=$$SRVD"
