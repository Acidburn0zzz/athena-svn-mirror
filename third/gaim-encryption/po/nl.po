msgid "Cancel"
msgstr "Annuleren"

#: config_ui.c:235 state_ui.c:287
msgid "Ok"
msgstr "Ok"

#: config_ui.c:267 config_ui.c:311 config_ui.c:492 config_ui.c:526
#: config_ui.c:553
msgid "Delete Key"
msgstr "Verwijder sleutel"

