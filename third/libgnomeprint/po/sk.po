msgid "/gnome-print.fontmap"
msgstr "/gnome-print.fontmap"

#: installer/gnome-font-install.c:159
msgid "Write output fontmap to specified file (- for stdout)"
msgstr ""
"Zapísať výstupnú mapu typov písma do zadaného súboru (- pre štandardný "
"výstup)"

#: installer/gnome-font-install.c:161
msgid ""
"Return the $FONTMAPDIR_STATIC and $FONTMAPDIR_DYNAMIC used for this gnome-"
"print installation)"
msgstr ""
"Vrátiť $FONTMAPDIR_STATIC a $FONTMAPDIR_DYNAMIC (použité pre túto inštaláciu "
"gnome-print)"

