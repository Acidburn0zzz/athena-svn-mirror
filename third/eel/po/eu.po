#, c-format
msgid ""
"GConf error:\n"
"  %s"
msgstr ""
"GConf errorea:\n"
"  %s"

#: eel/eel-gconf-extensions.c:87
#, c-format
msgid "GConf error: %s"
msgstr "GConf errorea: %s"

#: eel/eel-gconf-extensions.c:90
msgid "All further errors shown only on terminal."
msgstr "Gainerako erroreak terminalean bakarrik agertuko dira"

#: eel/eel-gconf-extensions.c:92
msgid "GConf Error"
msgstr "GConf-en errorea"

#. localizers: These strings are part of the strftime
#. * self-check code and must be changed to match what strtfime
#. * yields. The first one is "%m/%d/%y, %I:%M %p".
#.
#: eel/eel-glib-extensions.c:1162
msgid "01/01/00, 01:00 AM"
msgstr "01.01.00, 01:00:00 AM"

#. The second one is "%-m/%-d/%-y, %-I:%M %p".
#: eel/eel-glib-extensions.c:1164
msgid "1/1/00, 1:00 AM"
msgstr "00/1/1, 01:00 AM"

#. The third one is "%_m/%_d/%_y, %_I:%M %p".
#: eel/eel-glib-extensions.c:1166
msgid " 1/ 1/00,  1:00 AM"
msgstr " 00/ 1/ 1  1:00 AM"

