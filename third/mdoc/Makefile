# $Id: Makefile,v 1.1 1998-12-18 17:21:24 danw Exp $

SHELL=/bin/sh

TMACDIR=/usr/athena/share/tmac

all:

clean:

distclean:

check:

install:
	-mkdir -p ${DESTDIR}${TMACDIR}
	install -m 644 doc ${DESTDIR}${TMACDIR}/tmac.doc
	install -m 644 doc-common ${DESTDIR}${TMACDIR}/tmac.doc-common
	install -m 644 doc-ditroff ${DESTDIR}${TMACDIR}/tmac.doc-ditroff
	install -m 644 doc-nroff ${DESTDIR}${TMACDIR}/tmac.doc-nroff
	install -m 644 doc-syms ${DESTDIR}${TMACDIR}/tmac.doc-syms
