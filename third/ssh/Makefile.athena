# $Id: Makefile.athena,v 1.4 1998-04-30 20:08:12 ghudson Exp $

SHELL=/bin/sh

prepare:
	rm -f config.cache
	./configure --disable-suid-ssh \
		--with-kerberos5=${ATHTOOLROOT}/usr/athena \
		--enable-kerberos-tgt-passing \
		--with-rsh=/usr/athena/bin/rsh

clean:
	${MAKE} clean

all:
	${MAKE} depend all

check:

install:
	${MAKE} install install_prefix="$$SRVD"
