# $Id: Makefile.athena,v 1.3 1998-01-01 18:17:30 danw Exp $

SHELL=/bin/sh

prepare:
	rm -f config.cache
	./configure --disable-suid-ssh \
		--with-kerberos5=${ATHTOOLROOT}/usr/athena \
		--enable-kerberos-tgt-passing

clean:
	${MAKE} clean

all:
	${MAKE} depend all

check:

install:
	${MAKE} install install_prefix="$$SRVD"
