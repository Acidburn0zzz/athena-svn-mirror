SHELL=/bin/sh

prepare:
	imake "-I$$XCONFIGDIR" -DUseInstalled "-DXCONFIGDIR=$$XCONFIGDIR" \
		-DMWM_ONLY
	${MAKE} Makefiles

clean:
	${MAKE} clean

all:
	${MAKE} includes depend all

check:

install:
	${MAKE} install install.man DESTDIR="$$SRVD"
