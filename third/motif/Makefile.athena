# $Id: Makefile.athena,v 1.3 2000-03-29 19:55:48 ghudson Exp $

SHELL=/bin/sh

dist:

prepare:
	rm -f Makefile
	imake -Iconfig -DTOPDIR=`pwd` -DUseInstalled
	${MAKE} Makefiles

clean:
	${MAKE} clean

all:
	${MAKE} includes depend all TOP=`pwd`

check:

install:
	${MAKE} install DESTDIR="$$SRVD" TOP=`pwd`
