SHELL=/bin/sh
SUBDIRS=talk talkd

all:
	for i in ${SUBDIRS}; do (cd $$i; ${MAKE} $@) || exit 1; done

check:
	for i in ${SUBDIRS}; do (cd $$i; ${MAKE} $@) || exit 1; done

install:
	for i in ${SUBDIRS}; do (cd $$i; ${MAKE} $@) || exit 1; done

clean:
	for i in ${SUBDIRS}; do (cd $$i; ${MAKE} $@) || exit 1; done

distclean:
	for i in ${SUBDIRS}; do (cd $$i; ${MAKE} $@) || exit 1; done
