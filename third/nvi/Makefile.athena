# $Id: Makefile.athena,v 1.1 2000-03-04 19:43:48 ghudson Exp $

SHELL=/bin/sh

prepare:
	rm -f build/config.cache
	cd build && CONFIG_SITE= CFLAGS='-g -I/usr/include/db1' LIBS=-ldb1 \
		./configure --disable-curses --disable-db \
		--prefix=/usr --bindir=/bin

clean:
	cd build && ${MAKE} clean

all:
	cd build && ${MAKE} all

check:

install:
	cd build && ${MAKE} install DESTDIR="$$SRVD"
