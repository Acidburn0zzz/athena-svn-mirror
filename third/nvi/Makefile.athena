# $Id: Makefile.athena,v 1.6 2003-03-23 01:15:11 ghudson Exp $

SHELL=/bin/sh

dist:

prepare:
	rm -f build/config.cache
	cd build && CFLAGS=-g ./configure --disable-curses --prefix=/usr \
	  --bindir=/bin --mandir=/usr/share/man

clean:
	cd build && ${MAKE} clean

all:
	cd build && ${MAKE} all

check:

install:
	cd build && ${MAKE} install DESTDIR="$$SRVD"
