void        <link linkend="gnome-window-icon-set-default-from-file-list">gnome_window_icon_set_default_from_file_list</link>
                                            (const char **filenames);
void        <link linkend="gnome-window-icon-init">gnome_window_icon_init</link>          (void);
</synopsis>
