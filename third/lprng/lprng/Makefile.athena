# $Id: Makefile.athena,v 1.1 1999-02-22 03:45:13 danw Exp $

SHELL=/bin/sh

prepare:
	rm -f config.cache
	CC="$$COMPILER -I$(ATHTOOLROOT)/usr/athena/include" LDFLAGS=-L$(ATHTOOLROOT)/usr/athena/lib ./configure

clean:
	$(MAKE) clean

all:
	$(MAKE) CC="$$COMPILER -I$(ATHTOOLROOT)/usr/athena/include -I$(ATHTOOLROOT)/usr/athena/include/kerberosIV" LDFLAGS=-L$(ATHTOOLROOT)/usr/athena/lib

check:

install:
	$(MAKE) DESTDIR="$$SRVD" install
