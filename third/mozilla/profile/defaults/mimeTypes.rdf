                   NC:editable="true">
    <NC:handlerProp resource="urn:mimetype:handler:application/vnd.rn-realmedia"/>
  </RDF:Description>

  <RDF:Description about="urn:mimetype:handler:application/vnd.rn-realmedia"
                   NC:saveToDisk="false"
