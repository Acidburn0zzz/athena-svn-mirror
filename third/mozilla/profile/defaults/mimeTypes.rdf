                   NC:saveToDisk="false"
                   NC:handleInternal="false"
                   NC:alwaysAsk="true">
    <NC:externalApplication resource="urn:mimetype:externalApplication:application/x-java-jnlp-file"/>
  </RDF:Description>

  <RDF:Description about="urn:mimetype:externalApplication:application/x-java-jnlp-file"
                   NC:path="/usr/bin/javaws"
                   NC:prettyName="javaws" />

  <RDF:Description about="urn:mimetype:audio/x-pn-realaudio"
                   NC:value="audio/x-pn-realaudio"
                   NC:description=""
                   NC:editable="true">
    <NC:fileExtensions>rmm</NC:fileExtensions>
    <NC:fileExtensions>ram</NC:fileExtensions>
    <NC:handlerProp resource="urn:mimetype:handler:audio/x-pn-realaudio"/>
  </RDF:Description>

  <RDF:Description about="urn:mimetype:handler:audio/x-pn-realaudio"
                   NC:saveToDisk="false"
                   NC:handleInternal="false"
                   NC:alwaysAsk="true">
    <NC:externalApplication resource="urn:mimetype:externalApplication:audio/x-pn-realaudio"/>
