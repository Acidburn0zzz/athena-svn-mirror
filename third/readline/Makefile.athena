# $Id: Makefile.athena,v 1.1 2000-12-31 10:27:48 ghudson Exp $

dist:
	cp "$$CONFIG_SITE" config.site.athena

prepare:
	rm -f config.cache
	CONFIG_SITE=`pwd`/config.site.athena ./configure

clean:
	${MAKE} clean

all:
	${MAKE} all
	${MAKE} shared

check:

install:
	${MAKE} install DESTDIR="$$SRVD"
	cd shlib && ${MAKE} install DESTDIR="$$SRVD"
