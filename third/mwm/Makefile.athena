# $Id: Makefile.athena,v 1.3 1999-01-30 04:17:07 ghudson Exp $

SHELL=/bin/sh
ATHMANDIR=/usr/athena/man

prepare:
	imake "-I$$XCONFIGDIR" -DUseInstalled "-DCONFIGDIR=$$XCONFIGDIR"
	${MAKE} Makefiles

clean:
	${MAKE} clean

all:
	${MAKE} includes depend all

check:

install:
	${MAKE} install install.man DESTDIR="$$SRVD"
	mkdir -p "$$SRVD"${ATHMANDIR}/man1
	install -c -m 0444 clients/mwm/mwm.man "$$SRVD"${ATHMANDIR}/man1/mwm.1
