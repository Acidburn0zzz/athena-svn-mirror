# $Id: Makefile.athena,v 1.2 2002-02-03 16:18:07 ghudson Exp $

dist:
	cp "$$CONFIG_SITE" config.site.athena

prepare:
	rm -f config.cache
	. ./compiler && \
	  CC=$$cc CONFIG_SITE=`pwd`/config.site.athena ./configure

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	${MAKE} install DESTDIR="$$SRVD"
