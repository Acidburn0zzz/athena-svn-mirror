# $Id: Makefile.athena,v 1.4 2004-05-29 17:22:51 ghudson Exp $

dist:
	cp "$$CONFIG_SITE" config.site.athena

prepare:
	rm -f config.cache
	. ./compiler && CC=$$cc CONFIG_SITE=`pwd`/config.site.athena \
	  ./configure --disable-nls --program-transform-name="s/^/g/"

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	${MAKE} install DESTDIR="$$SRVD"
