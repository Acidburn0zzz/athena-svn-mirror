# $Id: Makefile.athena,v 1.3 2002-02-14 15:48:55 ghudson Exp $

dist:
	cp "$$CONFIG_SITE" config.site.athena

prepare:
	rm -f config.cache
	. ./compiler && CC=$$cc CONFIG_SITE=`pwd`/config.site.athena \
	  ./configure --program-transform-name="s/^/g/"

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	${MAKE} install DESTDIR="$$SRVD"
