<gconfschemafile>
    <schemalist>
      <schema>
        <key>/schemas/apps/gpdf/UI/Windows/show_sidebar</key>
        <applyto>/apps/gpdf/UI/Windows/show_sidebar</applyto>
        <owner>gpdf</owner>
        <type>bool</type>
        <default>0</default>
        <locale name="C">
        <short>Show sidebar by default</short>
        </locale>
      </schema>
      <schema>
        <key>/schemas/apps/gpdf/UI/Windows/sidebar_size</key>
        <applyto>/apps/gpdf/UI/Windows/sidebar_size</applyto>
        <owner>gpdf</owner>
        <type>int</type>
        <default>100</default>
        <locale name="C">
        <short>Default sidebar size</short>
        </locale>
      </schema>
      <schema>
	<key>/schemas/apps/gpdf/UI/Windows/sidebar_page</key>
	<applyto>/apps/gpdf/UI/Windows/sidebar_page</applyto>
	<owner>gpdf</owner>
	<type>string</type>
	<default>bookmarks</default>
        <locale name="C">
        <short>Default sidebar page</short>
        </locale>
      </schema>
    </schemalist>
</gconfschemafile>
