# $Id: Makefile.athena,v 1.5 1997-02-22 23:08:41 ghudson Exp $

SHELL=/bin/sh

prepare:
	rm -f .defaults
	cp "defaults.$$HOSTTYPE" .defaults
	module=`sed -n "s/^set module = '\(.*\)'$$/\1/p" .defaults`; \
	./Configure "$$module"

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	${MAKE} install-top install-man "DESTDIR=$$SRVD"
