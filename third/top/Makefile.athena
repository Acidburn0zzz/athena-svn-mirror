# $Id: Makefile.athena,v 1.6 1997-03-17 20:52:49 ghudson Exp $

SHELL=/bin/sh

prepare:
	rm -f .defaults
	cp "defaults.$$HOSTTYPE" .defaults
	module=`sed -n "s/^set module = '\(.*\)'$$/\1/p" .defaults`; \
	./Configure "$$module"

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install: always
	${MAKE} install "DESTDIR=$$SRVD"

always:
