# $Id: Makefile.athena,v 1.8 2001-04-03 16:06:47 ghudson Exp $

SHELL=/bin/sh

dist:

prepare:
	rm -f .defaults
	cp "defaults.$$ATHENA_HOSTTYPE" .defaults
	module=`sed -n "s/^set module = '\(.*\)'$$/\1/p" .defaults`; \
	./Configure "$$module"

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install: always
	${MAKE} install "DESTDIR=$$SRVD"

always:
