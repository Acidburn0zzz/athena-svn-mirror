# $Id: Makefile.athena,v 1.7 2000-03-29 19:54:40 ghudson Exp $

SHELL=/bin/sh

dist:

prepare:
	rm -f .defaults
	cp "defaults.$$HOSTTYPE" .defaults
	module=`sed -n "s/^set module = '\(.*\)'$$/\1/p" .defaults`; \
	./Configure "$$module"

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install: always
	${MAKE} install "DESTDIR=$$SRVD"

always:
