<refentry id="GnomeCanvasClipgroup">
<refmeta>
<refentrytitle>GnomeCanvasClipgroup</refentrytitle>
<manvolnum>3</manvolnum>
<refmiscinfo>LIBGNOMECANVAS Library</refmiscinfo>
