# $Id: Makefile.athena,v 1.5 2003-04-26 19:23:30 zacheiss Exp $

dist:

prepare:
	PATH=/afs/athena.mit.edu/software/sunsoft/SUNWspro/bin:${PATH} PERL=/usr/athena/bin/perl ./config shared --prefix=/usr/athena

clean:
	${MAKE} clean

all:
	${MAKE} PATH=/afs/athena.mit.edu/software/sunsoft/SUNWspro/bin:${PATH}

check:
	${MAKE} test

install:
	${MAKE} install INSTALL_PREFIX="$$SRVD"
