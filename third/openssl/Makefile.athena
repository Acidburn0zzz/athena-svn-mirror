# $Id: Makefile.athena,v 1.6 2004-10-26 16:25:08 ghudson Exp $

dist:

prepare:
	PATH=${sprobin}:$PATH PERL=/usr/athena/bin/perl ./config shared --prefix=/usr/athena

clean:
	${MAKE} clean

all:
	${MAKE} PATH=/afs/athena.mit.edu/software/sunsoft/SUNWspro/bin:${PATH}

check:
	${MAKE} test

install:
	${MAKE} install INSTALL_PREFIX="$$SRVD"
