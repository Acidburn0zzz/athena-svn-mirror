# $Id: Makefile.athena,v 1.9 2005-04-21 06:57:00 zacheiss Exp $

dist:

prepare:
	PATH=${sprobin}:${PATH} PERL=/usr/athena/bin/perl ./config shared --prefix=/usr/athena

clean:
	${MAKE} clean

all:
	LD_RUN_PATH=/usr/athena/lib ${MAKE} PATH=${sprobin}:${PATH}

check:
	${MAKE} test

install:
	LD_RUN_PATH=/usr/athena/lib ${MAKE} install INSTALL_PREFIX="$$SRVD" PATH=${sprobin}:${PATH}
