@HAVE_GTK_DOC_TRUE@all-local: html-build.stamp

#### scan ####

# wingo addition
@HAVE_GTK_DOC_TRUE@scan-build.stamp: $(HFILE_GLOB) $(SCANOBJ_DEPS)
@HAVE_GTK_DOC_TRUE@	@echo '*** Scanning header files ***'
@HAVE_GTK_DOC_TRUE@	if grep -l '^..*$$' $(srcdir)/$(DOC_MODULE).types > /dev/null ; then \
@HAVE_GTK_DOC_TRUE@	    CC="$(GTKDOC_CC)" LD="$(GTKDOC_LD)" CFLAGS="$(GTKDOC_CFLAGS)" LDFLAGS="$(GTKDOC_LIBS)" $(GTK_DOC_SCANOBJ) --module=$(DOC_MODULE) --output-dir=$(srcdir) ; \
@HAVE_GTK_DOC_TRUE@	else \
@HAVE_GTK_DOC_TRUE@	    cd $(srcdir) ; \
@HAVE_GTK_DOC_TRUE@	    for i in $(SCANOBJ_FILES) ; do \
@HAVE_GTK_DOC_TRUE@               test -f $$i || touch $$i ; \
@HAVE_GTK_DOC_TRUE@	    done \
@HAVE_GTK_DOC_TRUE@	fi
@HAVE_GTK_DOC_TRUE@	cd $(srcdir) && \
@HAVE_GTK_DOC_TRUE@	  gtkdoc-scan --module=$(DOC_MODULE) --source-dir=$(DOC_SOURCE_DIR) --ignore-headers="$(IGNORE_HFILES)" $(SCAN_OPTIONS) $(EXTRA_HFILES)
@HAVE_GTK_DOC_TRUE@	touch scan-build.stamp

@HAVE_GTK_DOC_TRUE@$(DOC_MODULE)-decl.txt $(SCANOBJ_FILES): scan-build.stamp
@HAVE_GTK_DOC_TRUE@	@true

#### templates ####

@HAVE_GTK_DOC_TRUE@tmpl-build.stamp: $(DOC_MODULE)-decl.txt $(SCANOBJ_FILES) $(DOC_MODULE)-sections.txt $(DOC_OVERRIDES)
@HAVE_GTK_DOC_TRUE@	@echo '*** Rebuilding template files ***'
@HAVE_GTK_DOC_TRUE@	cd $(srcdir) && gtkdoc-mktmpl --module=$(DOC_MODULE)
@HAVE_GTK_DOC_TRUE@	touch tmpl-build.stamp

@HAVE_GTK_DOC_TRUE@tmpl.stamp: tmpl-build.stamp
@HAVE_GTK_DOC_TRUE@	@true

#### sgml ####

@HAVE_GTK_DOC_TRUE@sgml-build.stamp: tmpl.stamp $(CFILE_GLOB)
@HAVE_GTK_DOC_TRUE@	@echo '*** Building SGML ***'
@HAVE_GTK_DOC_TRUE@	cd $(srcdir) && \
@HAVE_GTK_DOC_TRUE@	gtkdoc-mkdb --module=$(DOC_MODULE) --source-dir=$(DOC_SOURCE_DIR) --main-sgml-file=$(DOC_MAIN_SGML_FILE) $(MKDB_OPTIONS)
@HAVE_GTK_DOC_TRUE@	touch sgml-build.stamp

@HAVE_GTK_DOC_TRUE@sgml.stamp: sgml-build.stamp
@HAVE_GTK_DOC_TRUE@	@true

#### html ####

@HAVE_GTK_DOC_TRUE@html-build.stamp: sgml.stamp $(DOC_MAIN_SGML_FILE) $(content_files)
@HAVE_GTK_DOC_TRUE@	@echo '*** Building HTML ***'
@HAVE_GTK_DOC_TRUE@	test -d $(srcdir)/html || mkdir $(srcdir)/html
@HAVE_GTK_DOC_TRUE@	cd $(srcdir)/html && gtkdoc-mkhtml $(DOC_MODULE) ../$(DOC_MAIN_SGML_FILE)
@HAVE_GTK_DOC_TRUE@	test "x$(HTML_IMAGES)" = "x" || ( cd $(srcdir) && cp $(HTML_IMAGES) html )
@HAVE_GTK_DOC_TRUE@	@echo '-- Fixing Crossreferences' 
@HAVE_GTK_DOC_TRUE@	cd $(srcdir) && gtkdoc-fixxref --module-dir=html --html-dir=$(HTML_DIR) $(FIXXREF_OPTIONS)
@HAVE_GTK_DOC_TRUE@	touch html-build.stamp

clean-local:
	rm -f *~ *.bak $(SCANOBJ_FILES) *-unused.txt $(DOC_STAMPS)

maintainer-clean-local: clean
	cd $(srcdir) && rm -rf sgml html $(DOC_MODULE)-decl-list.txt $(DOC_MODULE)-decl.txt

# FIXME : thomas added all sgml files and some other things to make
# make distcheck work
distclean-local: clean
	rm -rf $(DOC_MODULE)-decl-list.txt
	rm -rf gstreamer-*.txt
	rm -rf tmpl/*.sgml
	rm -f gstreamer.hierarchy
	rm -f *.stamp || true

install-data-local:
	$(mkinstalldirs) $(DESTDIR)$(TARGET_DIR)
	(installfiles=`echo $(srcdir)/html/*.html`; \
	if test "$$installfiles" = '$(srcdir)/html/*.html'; \
	then echo '-- Nothing to install' ; \
	else \
	  for i in $$installfiles; do \
	    echo '-- Installing '$$i ; \
	    $(INSTALL_DATA) $$i $(DESTDIR)$(TARGET_DIR); \
	  done; \
	  echo '-- Installing $(srcdir)/html/index.sgml' ; \
	  $(INSTALL_DATA) $(srcdir)/html/index.sgml $(DESTDIR)$(TARGET_DIR); \
	fi)

#
# Require gtk-doc when making dist
#
@HAVE_GTK_DOC_TRUE@dist-check-gtkdoc:
@HAVE_GTK_DOC_FALSE@dist-check-gtkdoc:
@HAVE_GTK_DOC_FALSE@	@echo "*** gtk-doc must be installed and enabled in order to make dist"
@HAVE_GTK_DOC_FALSE@	@false

dist-hook: dist-check-gtkdoc dist-hook-local
	mkdir $(distdir)/tmpl
	mkdir $(distdir)/sgml
	mkdir $(distdir)/html
	-cp $(srcdir)/tmpl/*.sgml $(distdir)/tmpl
	-cp $(srcdir)/sgml/*.sgml $(distdir)/sgml
	-cp $(srcdir)/html/index.sgml $(distdir)/html
	-cp $(srcdir)/html/*.html $(srcdir)/html/*.css $(distdir)/html

	images=$(HTML_IMAGES) ;    	      \
	for i in $$images ; do		      \
	  cp $(srcdir)/$$i $(distdir)/html ;  \
	done

.PHONY : dist-hook-local
