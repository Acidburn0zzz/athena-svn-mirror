# $Id: Makefile.athena,v 1.2 1999-02-10 23:42:20 danw Exp $

SHELL=/bin/sh

prepare:
	rm -f config.cache
	./configure --enable-nmh-pop --enable-nmh-debug \
		--with-krb4=$(ATHTOOLROOT)/usr/athena \
		--with-hesiod=$(ATHTOOLROOT)/usr/athena

clean:
	${MAKE} clean

all:
	${MAKE} all

check:

install:
	${MAKE} install DESTDIR="$$SRVD"
