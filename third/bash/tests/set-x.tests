#!../bash
# $Id: set-x.tests,v 1.1.1.1 2005-01-19 13:21:26 zacheiss Exp $
#
# Test that "set -x" shows what we think it should.
#
set -x
for ((i=0; i<=5; i++ )) ; do
  x=0
done
for i in 0 1 2 ; do
  x=i
done
case x in 
 0) x=i ;;
 *) x=i ;;
esac

